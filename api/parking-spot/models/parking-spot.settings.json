{
  "kind": "collectionType",
  "collectionName": "parking_spots",
  "info": {
    "name": "Parking spot",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "dateStart": {
      "type": "datetime"
    },
    "dateTo": {
      "type": "datetime",
      "required": false
    },
    "car": {
      "model": "car"
    },
    "allowContact": {
      "type": "boolean",
      "default": false
    },
    "position": {
      "type": "component",
      "repeatable": false,
      "component": "parking.position"
    }
  }
}
